<root>
    <styles>
        <include src="file://{resources}/styles/custom_game/stats_container.css" />
    </styles>
    <scripts>
        <include src="file://{resources}/scripts/custom_game/stats_container.js" />
    </scripts>
    <Panel class="WoeStatsContainer">
        <Panel class="WoeStatsRow">
            <Panel class="WoeStatsCol">
                <Label class="WoeStatsIcon" text="MS"/>
                <Label class="WoeStatsNumber" id="WoeStatMSLabel" />
            </Panel>
            <Panel class="WoeStatsCol">
                <Label class="WoeStatsIcon" text="Haste" />
                <Label class="WoeStatsNumber" id="WoeStatHasteLabel" />
            </Panel>
        </Panel>
        <Panel class="WoeStatsRow">
            <Panel class="WoeStatsCol">
                <Label class="WoeStatsIcon" text="Armor" />
                <Label class="WoeStatsNumber" id="WoeStatArmorLabel" />
            </Panel>
            <Panel class="WoeStatsCol">
                <Label class="WoeStatsIcon" text="MR" />
                <Label class="WoeStatsNumber" id="WoeStatMRLabel" />
            </Panel>
        </Panel>
    </Panel>
</root>